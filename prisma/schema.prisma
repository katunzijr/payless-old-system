generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model api_clients {
  client_id     String   @id @db.VarChar(50)
  client_name   String   @db.VarChar(100)
  client_secret String?  @db.VarChar(255)
  is_active     Boolean? @default(true)
  rate_limit    Int?     @default(1000)
  created_at    DateTime @default(now()) @db.Timestamp(0)
  updated_at    DateTime @default(now()) @db.Timestamp(0)
  email         String?  @db.VarChar(255)
  phone         String?  @db.VarChar(20)
}

model ci_sessions {
  session_id    String @id @default("0") @db.VarChar(40)
  ip_address    String @default("0") @db.VarChar(45)
  user_agent    String @db.VarChar(120)
  last_activity Int    @default(0) @db.UnsignedInt
